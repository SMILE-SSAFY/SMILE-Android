<?xml version="1.0" encoding="utf-8"?>
<navigation
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_group"
    app:startDestination="@id/splashFragment">
    <fragment
        android:id="@+id/splashFragment"
        android:name="com.ssafy.smile.presentation.view.splash.SplashFragment"
        android:label="SplashFragment"
        tools:layout="@layout/fragment_splash" >
        <action
            android:id="@+id/action_splashFragment_to_logInFragment"
            app:destination="@id/logInFragment" />
        <action
            android:id="@+id/action_splashFragment_to_mainFragment"
            app:destination="@id/mainFragment" />
    </fragment>
    <fragment
        android:id="@+id/logInFragment"
        android:name="com.ssafy.smile.presentation.view.user.LogInFragment"
        android:label="fragment_log_in"
        tools:layout="@layout/fragment_log_in" >
        <action
            android:id="@+id/action_global_loginFragment"
            app:destination="@id/logInFragment"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim"
            app:popUpTo="@id/mainFragment"
            app:popUpToInclusive="true" />
        <action
            android:id="@+id/action_logInFragment_to_mainFragment"
            app:destination="@id/mainFragment"
            app:popUpTo="@id/logInFragment"
            app:popUpToInclusive="true" />
        <action
            android:id="@+id/action_logInFragment_to_signUpGraph"
            app:destination="@id/signUpGraph" />
    </fragment>
    <fragment
        android:id="@+id/mainFragment"
        android:name="com.ssafy.smile.presentation.view.MainFragment"
        android:label="fragment_main"
        tools:layout="@layout/fragment_main" >
        <argument
            android:name="photographerResponseDto"
            app:argType="com.ssafy.smile.data.remote.model.PhotographerResponseDto"
            app:nullable="true"
            android:defaultValue="@null" />
        <action
            android:id="@+id/action_mainFragment_to_findAddressFragment"
            app:destination="@id/findAddressFragment" />
        <action
            android:id="@+id/action_mainFragment_to_searchResultFragment"
            app:destination="@id/searchFragment" />
        <action
            android:id="@+id/action_mainFragment_to_customerReservationListFragment"
            app:destination="@id/customerReservationListFragment" />
        <action
            android:id="@+id/action_mainFragment_to_myInterestFragment"
            app:destination="@id/myInterestFragment" />
        <action
            android:id="@+id/action_mainFragment_to_photographerReservationList"
            app:destination="@id/photographerReservationList" />
        <action
            android:id="@+id/action_mainFragment_to_portfolioGraph"
            app:destination="@id/portfolioGraph">
            <argument
                android:name="photographerId"
                app:argType="long" />
            <argument
                android:name="postId"
                app:argType="long" />
        </action>
        <action
            android:id="@+id/action_mainFragment_to_registerPortFolioGraph"
            app:destination="@id/registerPortFolioGraph" >
            <argument
                android:name="photographerResponseDto"
                app:argType="com.ssafy.smile.data.remote.model.PhotographerResponseDto"
                app:nullable="true"
                android:defaultValue="@null" />
        </action>
        <action
            android:id="@+id/action_mainFragment_to_addressGraph"
            app:destination="@id/addressGraph" >
            <argument
                android:name="isSelectionMode"
                app:argType="boolean"
                android:defaultValue="true" />      <!-- TODO : Animation 추가 -->
        </action>
        <action
            android:id="@+id/action_mainFragment_to_mapListFragment"
            app:destination="@id/mapListFragment">
            <argument
                android:name="clusterId"
                app:argType="long"
                android:defaultValue="-1L"/>
        </action>
        <action
            android:id="@+id/action_mainFragment_to_recommendResultFragment"
            app:destination="@id/recommendResultFragment">
            <argument
                android:name="address"
                app:argType="string" />
        </action>
    </fragment>
    <fragment
        android:id="@+id/findAddressFragment"
        android:name="com.ssafy.smile.presentation.view.home.AddressSearchFragment"
        android:label="fragment_find_address"
        tools:layout="@layout/fragment_address_search" >
        <action
            android:id="@+id/action_findAddressFragment_to_setAddressFragment"
            app:destination="@id/setAddressFragment" />
    </fragment>
    <fragment
        android:id="@+id/searchFragment"
        android:name="com.ssafy.smile.presentation.view.home.SearchFragment"
        android:label="fragment_search"
        tools:layout="@layout/fragment_search" >
        <action
            android:id="@+id/action_searchFragment_to_portfolioGraph"
            app:destination="@id/portfolioGraph">
            <argument
                android:name="photographerId"
                app:argType="long" />
        </action>
        <action
            android:id="@+id/action_searchFragment_to_postDetailFragment"
            app:destination="@id/postDetailFragment">
            <argument
                android:name="postId"
                app:argType="long" />
        </action>
        <action
            android:id="@+id/action_searchFragment_pop"
            app:popUpTo="@id/searchFragment"
            app:popUpToInclusive="true" />
    </fragment>
    <fragment
        android:id="@+id/customerReservationListFragment"
        android:name="com.ssafy.smile.presentation.view.mypage.CustomerReservationListFragment"
        android:label="fragment_customer_reservation_list"
        tools:layout="@layout/fragment_customer_reservation_list">
        <action
            android:id="@+id/action_customerReservationListFragment_pop"
            app:popUpTo="@id/customerReservationListFragment"
            app:popUpToInclusive="true" />
        <action
            android:id="@+id/action_customerReservationListFragment_to_reviewDetailFragment"
            app:destination="@id/reviewDetailFragment">
            <argument
                android:name="photographerName"
                app:argType="string"
                app:nullable="true"
                android:defaultValue="@null" />
            <argument
                android:name="reservationId"
                app:argType="long"
                android:defaultValue="-1L" />
            <argument
                android:name="reviewId"
                app:argType="long"
                android:defaultValue="-1L" />
        </action>
    </fragment>
    <fragment
        android:id="@+id/myInterestFragment"
        android:name="com.ssafy.smile.presentation.view.mypage.myInterest.MyInterestFragment"
        android:label="fragment_my_interest"
        tools:layout="@layout/fragment_my_interest" >
        <action
            android:id="@+id/action_myInterestFragment_pop"
            app:popUpTo="@id/myInterestFragment"
            app:popUpToInclusive="true" />
        <action
            android:id="@+id/action_myInterestFragment_to_portfolioGraph"
            app:destination="@id/portfolioGraph" >
            <argument
                android:name="photographerId"
                app:argType="long" />
            <argument
                android:name="postId"
                app:argType="long" />
        </action>
    </fragment>
    <fragment
        android:id="@+id/photographerReservationList"
        android:name="com.ssafy.smile.presentation.view.mypage.PhotographerReservationListFragment"
        android:label="fragment_photographer_reservation_list"
        tools:layout="@layout/fragment_photographer_reservation_list" />
    <fragment
        android:id="@+id/setAddressFragment"
        android:name="com.ssafy.smile.presentation.view.home.AddressFragment"
        android:label="fragment_set_address"
        tools:layout="@layout/fragment_address" />
    <navigation
        android:id="@+id/signUpGraph"
        app:startDestination="@id/signUp1Fragment">
        <fragment
            android:id="@+id/signUp1Fragment"
            android:name="com.ssafy.smile.presentation.view.user.SignUp1Fragment"
            android:label="fragment_sign_up1"
            tools:layout="@layout/fragment_sign_up1" >
            <action
                android:id="@+id/action_signUp1Fragment_to_signUp2Fragment"
                app:destination="@id/signUp2Fragment">
                <argument
                    android:name="id"
                    app:argType="string" />
                <argument
                    android:name="password"
                    app:argType="string" />
            </action>
            <action
                android:id="@+id/action_signUp1Fragment_pop"
                app:popUpTo="@id/signUp1Fragment"
                app:popUpToInclusive="true" />
        </fragment>
        <fragment
            android:id="@+id/signUp2Fragment"
            android:name="com.ssafy.smile.presentation.view.user.SignUp2Fragment"
            android:label="fragment_sign_up2"
            tools:layout="@layout/fragment_sign_up2">
            <argument
                android:name="id"
                app:argType="string" />
            <argument
                android:name="password"
                app:argType="string" />
            <action
                android:id="@+id/action_signUp2Fragment_to_mainFragment"
                app:destination="@id/mainFragment"/>
            <action
                android:id="@+id/action_signUp2Fragment_pop"
                app:popUpTo="@id/signUp2Fragment"
                app:popUpToInclusive="true" />
        </fragment>
    </navigation>
    <navigation
        android:id="@+id/portfolioGraph"
        app:startDestination="@id/portfolioFragment">
        <fragment
            android:id="@+id/portfolioFragment"
            android:name="com.ssafy.smile.presentation.view.portfolio.PortfolioFragment"
            android:label="fragment_portfolio"
            tools:layout="@layout/fragment_portfolio" >
            <argument
                android:name="photographerId"
                app:argType="long"
                android:defaultValue="-1L"/>
            <argument
                android:name="postId"
                app:argType="long" />
            <action
                android:id="@+id/action_portfolioFragment_to_reservationFragment"
                app:destination="@id/reservationFragment">
                <argument
                    android:name="photographerId"
                    app:argType="long"/>
            </action>
            <action
                android:id="@+id/action_portfolioFragment_to_writePostFragment"
                app:destination="@id/writePostFragment" />
            <action
                android:id="@+id/action_portfolioFragment_to_postDetailFragment"
                app:destination="@id/postDetailFragment">
                <argument
                    android:name="postId"
                    app:argType="long" />
            </action>
            <action
                android:id="@+id/action_portfolioFragment_pop"
                app:popUpTo="@id/portfolioFragment"
                app:popUpToInclusive="true" />
            <action
                android:id="@+id/action_portfolioFragment_to_postDetailFragment_withPop"
                app:destination="@id/postDetailFragment"
                app:popUpTo="@id/portfolioFragment"
                app:popUpToInclusive="true" >
                <argument
                    android:name="postId"
                    app:argType="long" />
            </action>
        </fragment>
        <fragment
            android:id="@+id/postDetailFragment"
            android:name="com.ssafy.smile.presentation.view.portfolio.PostDetailFragment"
            android:label="fragment_post_detail"
            tools:layout="@layout/fragment_post_detail" >
            <argument
                android:name="postId"
                app:argType="long" />
            <action
                android:id="@+id/action_postDetailFragment_to_portfolioGraph"
                app:destination="@id/portfolioGraph" />
            <action
                android:id="@+id/action_postDetailFragment_to_postEditBottomSheetDialogFragment"
                app:destination="@id/postEditBottomSheetDialogFragment">
                <argument
                    android:name="postId"
                    app:argType="long" />
                <action
                    android:id="@+id/action_postEditBottomSheetDialogFragment_pop"
                    app:destination="@id/portfolioFragment" />
            </action>
            <action
                android:id="@+id/action_postDetailFragment_pop"
                app:popUpTo="@id/postDetailFragment"
                app:popUpToInclusive="true" />
        </fragment>
        <dialog
            android:id="@+id/postEditBottomSheetDialogFragment"
            android:name="com.ssafy.smile.presentation.view.portfolio.PostEditBottomSheetDialogFragment"
            android:label="fragment_post_edit_bottom_sheet_dialog"
            tools:layout="@layout/fragment_post_edit_bottom_sheet_dialog" >
            <argument
                android:name="postId"
                app:argType="long" />
        </dialog>
        <fragment
            android:id="@+id/reservationFragment"
            android:name="com.ssafy.smile.presentation.view.portfolio.ReservationFragment"
            android:label="fragment_reservation"
            tools:layout="@layout/fragment_reservation" >
            <argument
                android:name="photographerId"
                app:argType="long"/>
            <action
                android:id="@+id/action_reservationFragment_to_reservationResultFragment"
                app:destination="@id/reservationResultFragment">
                <argument
                    android:name="reservationResult"
                    app:argType="com.ssafy.smile.domain.model.CustomReservationDomainDto" />
            </action>
            <action
                android:id="@+id/action_reservationFragment_pop"
                app:popUpTo="@id/reservationFragment"
                app:popUpToInclusive="true" />
        </fragment>
        <fragment
            android:id="@+id/reservationResultFragment"
            android:name="com.ssafy.smile.presentation.view.portfolio.ReservationResultFragment"
            android:label="fragment_reservation_result"
            tools:layout="@layout/fragment_reservation_result" >
            <argument
                android:name="reservationResult"
                app:argType="com.ssafy.smile.domain.model.CustomReservationDomainDto" />
            <action
                android:id="@+id/action_reservationResultFragment_to_mainFragment"
                app:destination="@id/mainFragment" />
            <action
                android:id="@+id/action_reservationResultFragment_pop"
                app:popUpTo="@id/reservationResultFragment"
                app:popUpToInclusive="true" />
        </fragment>
        <fragment
            android:id="@+id/writePostFragment"
            android:name="com.ssafy.smile.presentation.view.portfolio.WritePostFragment"
            android:label="fragment_write_post"
            tools:layout="@layout/fragment_write_post" >
            <action
                android:id="@+id/action_writePostFragment_to_findAddressFragment"
                app:destination="@id/findAddressFragment" />
            <action
                android:id="@+id/action_writePostFragment_pop"
                app:popUpTo="@id/writePostFragment"
                app:popUpToInclusive="true" />
            <action
                android:id="@+id/action_writePostFragment_to_addressGraph"
                app:destination="@id/addressGraph">
                <argument
                    android:name="isSelectionMode"
                    app:argType="boolean"
                    android:defaultValue="false" />
            </action>
        </fragment>
    </navigation>
    <navigation
        android:id="@+id/registerPortFolioGraph"
        app:startDestination="@id/writePortfolioFragment">
        <argument
            android:name="photographerResponseDto"
            app:argType="com.ssafy.smile.data.remote.model.PhotographerResponseDto"
            app:nullable="true"
            android:defaultValue="@null" />
        <fragment
            android:id="@+id/writePortfolioFragment"
            android:name="com.ssafy.smile.presentation.view.mypage.PhotographerWritePortFolioFragment"
            android:label="fragment_write_portfolio"
            tools:layout="@layout/fragment_write_photographer_portfolio" >
            <action
                android:id="@+id/action_writePortfolioFragment_to_photographerProfileFragment"
                app:destination="@id/photographerProfileFragment" />
            <action
                android:id="@+id/action_writePortfolioFragment_pop"
                app:popUpTo="@id/writePortfolioFragment"
                app:popUpToInclusive="true" />
        </fragment>
        <dialog
            android:id="@+id/photographerProfileFragment"
            android:name="com.ssafy.smile.presentation.view.mypage.PhotographerProfileFragment"
            android:label="fragment_photographer_profile"
            tools:layout="@layout/fragment_photographer_profile" >
            <action
                android:id="@+id/action_photographerProfileFragment_pop"
                app:popUpTo="@id/photographerProfileFragment"
                app:popUpToInclusive="true" />
            <action
                android:id="@+id/action_photographerProfileFragment_to_photographerProfileCropFragment"
                app:destination="@id/photographerProfileCropFragment" />
        </dialog>
        <fragment
            android:id="@+id/photographerProfileCropFragment"
            android:name="com.ssafy.smile.presentation.view.mypage.PhotographerProfileCropFragment"
            android:label="PhotographerProfileCropFragment"
            tools:layout="@layout/fragment_image_crop">
            <action
                android:id="@+id/action_photographerProfileCropFragment_to_writePortfolioFragment"
                app:popUpTo="@id/writePortfolioFragment" />
            <action
                android:id="@+id/action_photographerProfileCropFragment_pop"
                app:popUpTo="@id/photographerProfileCropFragment"
                app:popUpToInclusive="true" />
        </fragment>
    </navigation>
    <navigation
        android:id="@+id/addressGraph"
        app:startDestination="@id/addressFragment">
        <dialog
            android:id="@+id/addressFragment"
            android:name="com.ssafy.smile.presentation.view.home.AddressFragment"
            android:label="AddressFragment"
            tools:layout="@layout/fragment_address">
            <argument
                android:name="isSelectionMode"
                app:argType="boolean"
                android:defaultValue="false" />
            <action
                android:id="@+id/action_addressFragment_to_addressSearchFragment"
                app:destination="@id/addressSearchFragment">
                <argument
                    android:name="isSelectionMode"
                    app:argType="boolean"
                    android:defaultValue="false" />
            </action>
            <action
                android:id="@+id/action_addressFragment_to_addressMapFragment"
                app:destination="@id/addressMapFragment" >
                <argument
                    android:name="isSelectionMode"
                    app:argType="boolean"
                    android:defaultValue="false" />
            </action>
            <action
                android:id="@+id/action_addressFragment_pop"
                app:popUpTo="@id/addressFragment"
                app:popUpToInclusive="true" />
        </dialog>
        <dialog
            android:id="@+id/addressSearchFragment"
            android:name="com.ssafy.smile.presentation.view.home.AddressSearchFragment"
            android:label="AddressSearchFragment"
            tools:layout="@layout/fragment_address_search">
            <argument
                android:name="isSelectionMode"
                app:argType="boolean"
                android:defaultValue="false" />
            <action
                android:id="@+id/action_addressSearchFragment_pop"
                app:popUpTo="@id/addressSearchFragment"
                app:popUpToInclusive="true" >
            </action>
            <action
                android:id="@+id/action_addressSearchFragment_pop_including_addressGraph"
                app:popUpTo="@id/addressGraph"
                app:popUpToInclusive="true" />
        </dialog>
        <dialog
            android:id="@+id/addressMapFragment"
            android:name="com.ssafy.smile.presentation.view.home.AddressMapFragment"
            android:label="fragment_address_map"
            tools:layout="@layout/fragment_address_map" >
            <argument
                android:name="isSelectionMode"
                app:argType="boolean"
                android:defaultValue="false" />
            <action
                android:id="@+id/action_addressMapFragment_pop"
                app:popUpTo="@id/addressMapFragment"
                app:popUpToInclusive="true" >
            </action>
            <action
                android:id="@+id/action_addressMapFragment_pop_including_addressGraph"
                app:popUpTo="@id/addressGraph"
                app:popUpToInclusive="true" />
        </dialog>
    </navigation>
    <fragment
        android:id="@+id/reviewDetailFragment"
        android:name="com.ssafy.smile.presentation.view.mypage.ReviewDetailFragment"
        android:label="fragment_review_detail"
        tools:layout="@layout/fragment_review_detail" >
        <argument
            android:name="photographerName"
            app:argType="string"
            app:nullable="true"
            android:defaultValue="@null" />
        <argument
            android:name="reservationId"
            app:argType="long"
            android:defaultValue="-1L" />
        <argument
            android:name="reviewId"
            app:argType="long"
            android:defaultValue="-1L" />
        <action
            android:id="@+id/action_reviewDetailFragment_pop"
            app:popUpTo="@id/reviewDetailFragment"
            app:popUpToInclusive="true" />
    </fragment>
    <fragment
        android:id="@+id/mapListFragment"
        android:name="com.ssafy.smile.presentation.view.map.MapListFragment"
        android:label="fragment_map_list"
        tools:layout="@layout/fragment_map_list" >
        <argument
            android:name="clusterId"
            app:argType="long"
            android:defaultValue="-1L" />
        <action
            android:id="@+id/action_mapListFragment_to_portfolioGraph"
            app:destination="@id/portfolioGraph">
            <argument
                android:name="postId"
                app:argType="long"
                android:defaultValue="-1L"/>
        </action>
    </fragment>
    <fragment
        android:id="@+id/recommendResultFragment"
        android:name="com.ssafy.smile.presentation.view.home.RecommendResultFragment"
        android:label="fragment_recommend_result"
        tools:layout="@layout/fragment_recommend_result">
        <argument
            android:name="address"
            app:argType="string" />
        <action
            android:id="@+id/action_recommendResultFragment_pop"
            app:popUpTo="@id/recommendResultFragment"
            app:popUpToInclusive="true" />
        <action
            android:id="@+id/action_recommendResultFragment_to_mainFragment"
            app:popUpTo="@id/mainFragment" />
    </fragment>

</navigation>